{
  "name": "multi-command",
  "displayName": "multi-command",
  "description": "Open multiple configurable windows at once (for repetitive tasks)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:multi-command.openMultipleTerminals"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multi-command.openMultipleTerminals",
        "title": "Open Multiple Terminals"
      }
    ],
    "keybindings": [
      {
        "command": "multi-command.openMultipleTerminals",
        "key": "cmd+shift+r",
        "mac": "cmd+shift+r",
        "linux": "ctrl+shift+r",
        "win": "ctrl+shift+r"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.8.2",
    "typescript": "^4.9.5"
  }
}
